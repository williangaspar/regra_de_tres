<?xml version='1.0' encoding='utf-8'?>
<widget id="com.williangaspar.regra_de_tres" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Regra de Três</name>
    <description>
        Simples aplicativo para regra de três
    </description>
    <author email="williangaspar2017@gmail.com" href="https://www.facebook.com/williamGasparApps">
        William Gaspar Apps
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="browser" spec="^4.1.0" />
    <plugin name="cordova-plugin-browsersync" spec="^0.1.6" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
</widget>
